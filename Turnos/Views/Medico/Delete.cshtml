@model Turnos.Models.Medico



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Eliminar</title>
</head>
<body>

<h4>Seguro desea eliminar el siguiente registro?</h4>
<div>
    <h5>Medico</h5>
    <hr />
    <div class="row" asp-action="Delete">
        <div class="col s12 m6">
            <div class="card blue darken-1 hoverable">
                <div class="card-content white-text">
                    <span class="card-title"><h5><b></b>@Html.DisplayFor(model => model.Nombre), @Html.DisplayFor(model => model.Apellido)</b></b></h5></span>
                    <br />
                    <dt>
                        @Html.DisplayNameFor(model => model.Direccion): @Html.DisplayFor(model => model.Direccion)
                    </dt>
                    <hr />
                    <dt>
                        @Html.DisplayNameFor(model => model.Telefono): @Html.DisplayFor(model => model.Telefono)
                    </dt>
                    <hr />
                    <dt>
                        @Html.DisplayNameFor(model => model.Email): @Html.DisplayFor(model => model.Email)
                    </dt>
                    <hr />
                    <dt>
                        @Html.DisplayNameFor(model => model.HorarioAtencionDesde): @Html.DisplayFor(model => model.HorarioAtencionDesde)
                    </dt>
                    <hr />
                    <dt>
                        @Html.DisplayNameFor(model => model.HorarioAtencionHasta): @Html.DisplayFor(model => model.HorarioAtencionHasta)
                    </dt>
                   
                </div>
                <div class="card-action blue darken-3" >
                    <form asp-action="Delete">
                        <input type="hidden" asp-for="IdMedico" />
                        <input type="submit" class="waves-effect waves-light btn-small  red lighten-2 " value="ELIMINAR" />
                        <a class="white-text right" asp-action="Index"><b>VOLVER </b></a>
                    </form>
                </div>
            </div>
        </div>
    </div>
  

</div>
</body>
</html>
