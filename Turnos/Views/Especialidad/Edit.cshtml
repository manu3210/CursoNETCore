@model Turnos.Models.Especialidad

<h5>Especialidad</h5>

<hr />
<br />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly"></div>
            <input type="hidden" asp-for="IdEspecialidad" />
            <div class="row">
                <div class="input-field col s6">
                    <i class="material-icons prefix">fact_check</i>
                    <input asp-for="Descripcion" type="text" class="validate">
                    <label asp-for="Descripcion"></label>
                    <span asp-validation-for="Descripcion"></span>
                </div>

            </div>

            <div class="row">
                <div class="col s6 m6">
                    <button class="btn waves-effect waves-light blue darken-3" type="submit" name="action">
                        Enviar
                        <i class="material-icons right">send</i>
                    </button>
                </div>
                <div class="col s6 m6">
                    <a class="waves-effect waves-light btn grey darken-2 right" asp-action="Index"><i class="material-icons left">undo</i>Volver</a>
                </div>
            </div>

        </form>

        
    </div>
    
</div>          

           
